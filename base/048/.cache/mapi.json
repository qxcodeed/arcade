            "name": "lib.cpp",
            "contents": "#include \"lib.hpp\"\n#include <sstream>\n#include <functional>\n\nBTree::~BTree(){\n\n    std::function <void (Node*)>\n    destroy = [&](Node * node) {\n        if(node == nullptr)\n            return;\n        destroy(node->left);\n        destroy(node->right);\n        delete node;\n    };\n\n    destroy(this->root);\n}\n\n\nstd::string BTree::in_order() {\n    std::stringstream os;\n\n    std::function <void (Node*)>\n    walk = [&](Node* node) {\n        if(node == nullptr)\n            return;\n        walk(node->left);\n        os << node->value << \" \";\n        walk(node->right);\n    };\n\n    os << \"[ \";\n    walk(root);\n    os << \"]\";\n\n    return os.str();\n}\n\n\nstd::string BTree::format(){\n    std::ostringstream os;\n\n    std::function <void (Node*, std::string)> \n    walk = [&](Node * node, string path) {\n        if(node != nullptr && (node->left != nullptr || node->right != nullptr))\n            walk(node->left , path + \"l\");\n        for(int i = 0; i < (int) path.size() - 1; i++)\n            os << (path[i] != path[i + 1] ? \"\u2502   \" : \"    \");\n        if(path != \"\")\n            os << (path.back() == 'l' ? \"\u250c\u2500\u2500\u2500\" : \"\u2514\u2500\u2500\u2500\");\n        if(node == nullptr){\n            os << \"#\" << endl;\n            return;\n        }\n        os << node->value << endl;\n        if(node != nullptr && (node->left != nullptr || node->right != nullptr))\n            walk(node->right, path + \"r\");\n    };\n\n    walk(this->root, \"\");\n    return os.str();\n}\n\n\n\n    \n\n\n",
