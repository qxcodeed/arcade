            "name": "lib.hpp",
            "contents": "#pragma once\n#include <iostream>\nusing namespace std;\n\nstruct Node {\n    int value;\n    Node * left;\n    Node * right;\n    Node(int value = 0, Node * left = nullptr, Node * right = nullptr){\n        this->value = value;\n        this->left = left;\n        this->right = right;\n    }\n};\n\nclass BTree {\npublic:\n    Node * root {nullptr};\n    BTree(string serial);\n    ~BTree();\n    std::string format();\n    std::string in_order();\n};",
